cumentation>if required it must contain the name of the classification defined by the owner (see "name")----------------------ENTITY IfcRootDescription	:IfcText;</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:boolean">						<xs:pattern value="true"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="type_classTypeEnum">		<xs:choice>			<xs:element name="allowedValue">				<xs:annotation>					<xs:documentation>if TRUE any of the allowed enumeration values can be assigned</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:boolean">						<xs:pattern value="true"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="enumValue">				<xs:annotation>					<xs:documentation>indicates the required enumeration value</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:pattern value="\.[A-Z0-9]+\."/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="objectType" type="xs:string">				<xs:annotation>					<xs:documentation>if objectType is defined, the value of enumValue must be equal to .USERDEFINED.</xs:documentation>				</xs:annotation>			</xs:element>		</xs:choice>		<xs:attribute name="referenceTo_bSDD"/>		<xs:attribute name="referenceToClassification" type="xs:anyURI" use="optional"/>		<xs:attribute name="referenceToDocumentation" type="xs:anyURI" use="optional"/>	</xs:complexType>	<xs:complexType name="type_classification">		<xs:sequence>			<xs:element name="ID_classification" type="type_ID">				<xs:annotation>					<xs:documentation>reference to the ID of a classification(unique identifier of the classification defined in the classificationTables)</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="code" type="type_ID" minOccurs="0">				<xs:annotation>					<xs